{
  "name": "compare-str",
  "version": "0.1.0",
  "type": "module",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "description": "Utility library for string comparison",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run clean && tsup --format esm ./src/index.ts --dts --sourcemap --minify --clean && echo 'export {};' >> dist/index.d.ts",
    "prepare": "npm run build && husky install",
    "clean": "rm -rf dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alancj731/stringcomp.git"
  },
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.0",
    "husky": "^9.1.7",
    "tsup": "^8.5.1",
    "typescript": "^5.5.0"
  }
}
